int x=100;
int y=45;
void setup() {
  size(1000, 1000);
}
void draw() {
  background(204);
  if (keyPressed&&(key==CODED)) {
    if (keyCode==LEFT) {
      x--;
    }
    if (keyCode==RIGHT) {
      x++;
    }
    if (keyCode==UP) {
      y--;
    }
    if (keyCode==DOWN) {
      y++;
    }
    if (keyCode==ALT) {
      fill(255, 0, 0);
    }
  }
  rect(x, y, 50, 50);
}
